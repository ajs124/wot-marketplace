{
    "name":"journal",
    "github": 25,
    "type": "template",
    "td": {
        "id": "urn:dev:wot:com:example:servient:lamp",
        "name": "MyLampThing",
        "securityDefinitions": {
            "basic_sc": {
                "scheme": "basic",
                "in": "header"
            }
        },
        "security": ["basic_sc"],
        "properties": {
            "status": {
                "type": "string",
                "forms": [{
                    "href": "https://mylamp.example.com/status"
                }]
            }
        },
        "actions": {
            "toggle": {
                "forms": [{
                    "href": "https://mylamp.example.com/toggle"
                }]
            }
        },
        "events": {
            "overheating": {
                "data": {
                    "type": "string"
                },
                "forms": [{
                    "href": "https://mylamp.example.com/oh",
                    "subprotocol": "longpoll"
                }]
            }
        }

    }
}